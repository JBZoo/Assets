{
    "name"              : "jbzoo/assets",
    "type"              : "library",
    "description"       : "Manage any asset files (js, css, less) and their depending + compressing and merging",
    "license"           : "MIT",
    "keywords"          : ["javascript", "js", "css", "less", "assets", "compress"],
    "minimum-stability" : "stable",
    "authors"           : [
        {
            "name"  : "SmetDenis",
            "email" : "denis@jbzoo.com",
            "role"  : "lead"
        },
        {
            "name"  : "Sergey Kalistratov",
            "email" : "kalistratov.s.m@gmail.com"
        }
    ],

    "require"           : {
        "php"         : ">=7.1",
        "jbzoo/utils" : "^3.0",
        "jbzoo/path"  : "^2.0",
        "jbzoo/less"  : "^2.0",
        "jbzoo/data"  : "^3.0"
    },

    "require-dev"       : {
        "jbzoo/phpunit"      : "^3.0",
        "jbzoo/profiler"     : "^1.0",
        "symfony/filesystem" : "^4.4"
    },

    "autoload"          : {
        "psr-4" : {
            "JBZoo\\Assets\\" : "src"
        }
    },

    "autoload-dev"      : {
        "classmap" : ["tests"]
    },

    "config"            : {
        "optimize-autoloader" : true
    },

    "support"           : {
        "issues" : "https://github.com/JBZoo/Assets/issues"
    }
}
